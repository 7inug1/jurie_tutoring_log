{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "jquery"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "jasmine": true,
    "jest": true
  },
  "rules": {
    "no-eq-null": "error",
    "no-empty": "error",
    "no-constant-condition": 1,
    "no-undef": 0,
    "no-console": 1,
    "space-before-blocks": "off",
    "space-in-parens": "off",
    "no-spaced-func": "off",
    "lines-around-comment": "off",
    "array-bracket-spacing": "off",
    "computed-property-spacing": "off",
    "max-len": "off",
    "no-unused-vars": "off",
    "no-nested-ternary": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "tabWidth": 2,
        "printWidth": 300
      }
    ]
  }
}